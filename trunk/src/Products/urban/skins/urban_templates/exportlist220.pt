      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xml:lang="en" lang="en"
      i18n:domain="urban"
      metal:use-macro="here/main_template/macros/master">


<metal:block fill-slot="top_slot"
             tal:define="dummy python:request.set('disable_border',1)" />
<head>
    <title i18n:translate="urban_export_list220">Export list 220</title>
</head>

<body>
<div metal:fill-slot="main">
<!-- dÃ©finition des variables -->

<h1 class="documentFirstHeading" i18n:translate="urban_export_list220">Export list 220</h1>

<form action="urbainxml" method="GET">
<span i18n:translate="label_from">From</span> : <input type="text" name="datefrom">
<span i18n:translate="label_to">To</span> : <input type="text" name="dateto"><br>
<input type="checkbox" name="list_only" value=1>
