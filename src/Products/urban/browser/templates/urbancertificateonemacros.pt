    <table class="no-style-table" cellspacing=0 cellpadding=0 width=100%>
     <tr>
      <td valign="top"><b><span i18n:translate="urban_label_receiptDate">Receipt date</span>:</b></td>
      <td width="2%"></td>
      <td valign="top" tal:define="receiptDate view/getReceiptDate">
       <a href="#" tal:condition="receiptDate" tal:attributes="href python: receiptDate['url']" tal:content="python: receiptDate['date']">Content</a>
       <span class="discreet" tal:condition="not: receiptDate" i18n:translate="content_none">None</span>
      </td>
     </tr>
     <tr>
      <td valign="top"><b><span i18n:translate="urban_label_theLicenceDate">The licence date</span>:</b></td>
      <td width="2%"></td>
      <td valign="top" tal:define="theLicenceDate view/getTheLicenceDate">
       <a href="#" tal:condition="theLicenceDate" tal:attributes="href python: theLicenceDate['url']" tal:content="python: theLicenceDate['date']">Content</a>
       <span class="discreet" tal:condition="not: theLicenceDate" i18n:translate="content_none">None</span>
      </td>
     </tr>
    </table>
   </fieldset>
  </td>
  </tr>
 <tr>
  <td valign="top" width=50%>
   <fieldset><b><legend i18n:translate="legend_details">Details</legend></b>
    <table class="no-style-table" cellspacing=0 cellpadding=0 width=100%>
