<div metal:define-macro="fullmapCSS" tal:define="staticUrbanmapHostHost context/portal_urban/getStaticPylonsHost">
		<style>
            #spwViewerDiv {
                position: absolute;
                top: 0px;
                bottom: 0px;
                height: 100%;
                width: 100%;
                overflow: hidden;
            }
            #map {
                position:absolute;
                width: 100%;
                height: 100%;
                margin: 0;
                padding: 0;
            }
			.draw-point {
				text-align: center;
				padding: 10px;
			}
		</style>

</div>

<div metal:define-macro="parcelmapCSS" tal:define="staticUrbanmapHostHost context/portal_urban/getStaticPylonsHost">
		<style>
            #spwViewerDiv {
                position: relative;
                top: 0px;
                bottom: 0px;
                height: 800px;
                width: 100%;
                overflow: hidden;
            }
			.draw-point {
				text-align: center;
				padding: 10px;
			}
		</style>

</div>

<div metal:define-macro="spwMapConfigParcel" tal:define="staticUrbanmapHostHost context/portal_urban/getStaticPylonsHost">
		<script tal:content='python: view.getMapConfig()'></script>
</div>

