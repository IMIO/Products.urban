<metal:block use-macro="here/global_defines/macros/defines" />
<html i18n:domain="urban">
<head>
<link tal:replace="structure provider:plone.resourceregistries.styles"/>
<link tal:replace="structure provider:plone.resourceregistries.scripts"/>
<link tal:replace="structure provider:plone.resourceregistries.kineticstylesheets"/>

</head>

<body onload="activateCollapsibles()">

<tal:block define="tmpcapakeys request/capakey|nothing; capakeys python: hasattr(tmpcapakeys, 'isalpha') and [tmpcapakeys,] or tmpcapakeys" repeat="capakey capakeys">

<div tal:define="parcel python: context.portal_urban.getParcelInfos(capakey)">
      <dl class="collapsible expandedOnLoad" tal:condition="parcel">
        <dt class="collapsibleHeader" tal:content="python:parcel['name']">name</dt>
        <dd class="collapsibleContent">
        <table border=0>
            <tr>
                <td><b><u i18n:translate="urban_label_owner">owner:</u></b> </td>
                <td>
                    <span tal:content="python:parcel['ownername']"/><br>
                    <span tal:content="python:parcel['ownerstreet']"/><br>
                    <span tal:content="python:parcel['ownercity']"/><br>
                </td>
            </tr>
            <tr>
                <td><b><u i18n:translate="urban_label_type">type:</u></b> </td>
                <td>
                    <span tal:content="python:parcel['type']"/><br>
                </td>
            </tr>
            </table>
                    <table tal:define="brains python:here.portal_urban.searchByParcel(foldertypes=['BuildLicence','Declaration','ParcelOutLicence'],division=parcel['division'],section=parcel['section'],radical=parcel['radical'],bis=parcel['bis'],exposant=parcel['exposant'],puissance=parcel['puissance'],partie='')" class="listing" width=100% border=0 cellspacing=0>
 <thead>
  <tr>
   <th><strong><span i18n:translate="urban_label_concerned_elements">Concerned elements</span> :</strong></th>
  </tr>
 </thead>
 <!-- Display 'no results' -->
 <tbody tal:condition="not: brains">
  <tr>
   <td i18n:domain="atcontenttypes" i18n:translate="description_no_items_in_topic" class="discreet">No result found.
   </td>
  </tr>
 </tbody>
 <!-- Display the results body -->
 <tbody>
  <tr tal:repeat="brain brains">
   <td tal:define="oddrow repeat/brain/odd;
                   obj python:brain.getObject();
                   obj_wf_state obj/review_state|python: wtool.getInfoFor(obj, 'review_state', '');
                   obj_wf_state_class python:'state-' + normalizeString(obj_wf_state);" tal:attributes="class python: test(oddrow, 'even', 'odd')">
    <img src="#" tal:attributes="src brain/getIcon" />
    <a href="#" tal:content="brain/Title" tal:attributes="href brain/getPath;class obj_wf_state_class">Result title</a>
    <p class="discreet"><span tal:content="structure brain/Description">Result description</span></p>
   </td>
  </tr>
 </tbody>
</table>
              
            
        
        </dd>
      </dl>
</div>


</tal:block>



</body>
</html>
