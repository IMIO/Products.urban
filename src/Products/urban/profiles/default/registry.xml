    <record name="collective.fingerpointing.interfaces.IFingerPointingSettings.audit_iterate" interface="collective.fingerpointing.interfaces.IFingerPointingSettings" field="audit_iterate">
      <field type="plone.registry.field.Bool">
        <default>True</default>
        <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="collective.fingerpointing" ns0:translate="">If plone.app.iterate is installed, log events like content editing checkouts and checkins.</description>
        <title xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="collective.fingerpointing" ns0:translate="">Audit Iterate?</title>
      </field>
      <value>False</value>
    </record>
    <record name="collective.fingerpointing.interfaces.IFingerPointingSettings.audit_lifecycle" interface="collective.fingerpointing.interfaces.IFingerPointingSettings" field="audit_lifecycle">
      <field type="plone.registry.field.Bool">
        <default>True</default>
        <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="collective.fingerpointing" ns0:translate="">Log content type life cycle events like object creation, modification and removal.</description>
        <title xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="collective.fingerpointing" ns0:translate="">Audit Content Type Life Cycle?</title>
